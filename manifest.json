{
    "name": "Page Evaluation Extension",
    "description" : "Enable you to evaluate pages in the search results page.",
    "version": "0.0",
    "manifest_version": 2,
    "permissions":[
        "storage", "tabs"
    ],
    "browser_action":{
        "default_title": "Page Evaluation",
        "default_popup": "services/popup.html"
    },
    "content_scripts":[
        {
            "matches":[
                "https://www.google.com/search*"
            ],
            "js":[
                "site-packages/jquery-3.5.1.min.js",
                "site-packages/jquery.raty.js",
                "services/content.js"
            ],
            "css":[
                "site-packages/bootstrap.min.css"
            ]
        }
    ]
  }